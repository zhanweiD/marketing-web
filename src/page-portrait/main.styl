.portrait-wrap
  height 100%
  box-sizing border-box
  overflow hidden
  position relative

  .noData-box
    height calc(100vh - 141px)
    margin 16px
  .resultList
    // position relative
    .ant-tabs-nav-wrap
      justify-content center
  .ant-tabs-nav
    background-color #fff
  .herder
    font-size 14px
    color rgba(0,0,0,0.85)
    line-height 56px
    padding-left 16px
    border-bottom 1px solid #E6EDF8
  .search
    // width 100%
    margin-bottom 0px
    .ant-tabs-nav
      background-color #1890ff
      padding-bottom 16px
    .ant-tabs-nav-operations
      display none
    .ant-tabs-tab
      border-radius 2px !important
      margin-right 24px
      font-size 12px
    // 搜素框
    .search_content
      padding-top 24px
      padding-bottom 24px
      background-color #fff
      font-size 12px 
      color rgba(0,0,0,0.65)
      display flex
      justify-content center
      background-color #1890ff
  .user-info
    background-color #f3f4f9
    height calc(100vh - 141px)
    
    .basis-info
      width 20%
      overflow auto

      &-content
        height 100%
        &-list
          width 100%
          .info-content
            display flex
            justify-content: space-between
            font-size 12px
            .p-tag
              color rgba(0,0,0,.45)
            .p-val
              color rgba(0,0,0,.65)
          .info-title
            color rgba(0,0,0,.65)
            text-align left
            margin-bottom 8px
            margin-top 8px
            display flex
            justify-content space-between
            align-items center
            .title
              margin-right 8px
              color #2466ff
            .line
              flex 1
              height 1px
              background-color #ccc
    .user-portrait
      width 55%

      .chart
        width 50%
        position relative
        .tag-herder
          color rgba(0,0,0,.85)
        .no-Data
          position absolute
          top 138px
          left 0px
          width 100%
      .evaluation-cloud
        width 50%
        .evaluation-herder
          font-size 14px
          color rgba(0,0,0,.85)
        .evaluation-cloud-content
          position relative
          .no-Data
            position absolute
            top 0
            left 0px
            width 100%
            padding-top 100px

      .object-cloud
        background #FFFFFF
        border 1px solid #E6EDF8
        border-radius 2px
        margin-bottom 16px
        height 450px

        &-header
          line-height 56px
          padding-left 16px
          font-size 14px
          color rgba(0, 0, 0, .85)
          border-bottom 1px solid #E6EDF8

        &-count
          font-size 12px
          color rgba(0, 0, 0, .45) 

        &-content
          height 360px
          overflow hidden
          position relative
          padding 16px 60px

        .tag-herder
          font-size 14px
          color rgba(0,0,0,.85)

        .noData
          position absolute
          top 0
          left 0px
          width 100%
          padding-top 100px
    .business-contact
      width 25%
      .time-list
        height calc(100vh - 232px)
        overflow-y auto
        overflow-x hidden
      // .yichu
      //   overflow:hidden;
      //   text-overflow:ellipsis;
      //   white-space:nowrap
      .ant-menu-submenu-title
        padding-left 0 !important
        line-height 24px
        height 24px
        font-size 12px
        margin 0
        margin-top -2px
      .ant-timeline-item
        padding-bottom 0
      .ant-timeline.ant-timeline-label .ant-timeline-item-label, .ant-timeline-item-content
        top -4px


