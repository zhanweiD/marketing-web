.dag-process
  .dag-header
    .containerStyle
      margin-right 8px
      line-height 50px 
      text-align center

    .dag-drag-box
      user-select none
      border solid 1px #49aede
      margin 8px
      padding 2px
      line-height 1
  
  .dag-style
    position absolute
    top 72px
    left 0
    right 0
    bottom 0
    border solid 1px #dbeff8
    backgroundColor #fff

    .dag-node
      max-width 240px
      word-break break-word
      white-space normal
      line-height: 1.2
      padding 4px 0