@import '../../node_modules/antd/dist/antd.css'
@import './antd-cover.styl'
.page
  min-height calc(100vh - 0px)
  background-color #f3f4f9
  padding 16px
  .page-main
    background-color #fff
    min-height calc(100vh - 32px)

  .bottom-button
    position absolute
    left 0
    bottom 0
    width 100%
    border-top 1px solid #e9e9e9
    padding 10px 16px
    background #fff
    text-align center